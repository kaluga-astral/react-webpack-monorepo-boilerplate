{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "config": [
        "./src/config"
      ],
      "config/*": [
        "./src/config/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.js",
    "./**/*.jsx",
    "./**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "cypress"
  ]
}
